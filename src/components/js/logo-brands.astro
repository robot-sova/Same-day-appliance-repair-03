<script defer>var logoElements=document.querySelectorAll("div[class^='logo-bran-ph-'], div[class*=' logo-bran-ph-']");var logoElementsLength=logoElements.length;for(let i=1;i<=logoElementsLength;i++){const logoBranPh=document.querySelector(`.logo-bran-ph-${i}`);const logoBranDLink=document.querySelector(`.logo-bran-d-${i} a`);logoBranPh.addEventListener('mouseover',function(){logoBranDLink.classList.add('hovered');logoBranPh.classList.add('hovered')});logoBranPh.addEventListener('mouseout',function(){logoBranDLink.classList.remove('hovered');logoBranPh.classList.remove('hovered')});const logoBranD=document.querySelector(`.logo-bran-d-${i}`);logoBranD.addEventListener('mouseover',function(){logoBranPh.querySelector('img').classList.add('hovered');logoBranPh.classList.add('hovered')});logoBranD.addEventListener('mouseout',function(){logoBranPh.querySelector('img').classList.remove('hovered');logoBranPh.classList.remove('hovered')})}</script>